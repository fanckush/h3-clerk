{
  "name": "h3-clerk",
  "version": "0.2.2",
  "packageManager": "pnpm@8.6.11",
  "description": "Unofficial Clerk middleware for H3.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/h3-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/h3-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/h3-clerk/issues",
  "keywords": [
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "h3"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "h3": "^1.7.0"
  },
  "dependencies": {
    "@clerk/backend": "^0.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@types/supertest": "^2.0.12",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "h3": "^1.7.1",
    "supertest": "^6.3.3",
    "taze": "^0.11.2",
    "tsup": "7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1",
    "vitest-fetch-mock": "^0.2.2"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "n/prefer-global/process": "off"
    }
  }
}
